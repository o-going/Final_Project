        var children = (obj.children) ? obj.children : obj._children; // childrenì´ í´ì ¸ìˆëŠ” ì• ë“¤ì´ë©´ children, ì ‘í˜€ìˆëŠ” ì• ë“¤ì´ë©´ _children
        let flag = true;
        for(var i=0; i<children.length; i++){ 
          if (children[i].class == 'found') { // childrenì˜ classê°€(ì„ íƒë˜ì–´) foundë©´
            search_select_node(children[i],path); // ì¬ê·€ë¡œ childrenì„ ê³„ì† ë”
            // flag = false; // flagë¥¼ falseë¡œ ë³€í™˜(ì—†ìœ¼ë©´) => ê±¸ë¦¼
          }
        }
        if (flag) { 
          if(obj.name != root) {
            path.push(obj); // ë§Œì•½ classê°€ foundë©´ pathì— í•´ë‹¹ childrenì„ ë„£ì–´ì¤Œ
          }
          // path.push(obj); // pathì— í•´ë‹¹ childrenì„ ë„£ì–´ì¤Œ
        }
      }else {
        if(obj.class == 'found') {
          if(obj.name != root) {
            path.push(obj); // ë§Œì•½ classê°€ foundë©´ pathì— í•´ë‹¹ childrenì„ ë„£ì–´ì¤Œ
          }
        }
      }
    }
  
    function extract_select2_data(node,leaves,index) {
            if (node.children){
                for(var i = 0; i<node.children.length; i++){
                    index = extract_select2_data(node.children[i],leaves,index)[0];
                }
            }
            else {
                leaves.push({id:++index,text:node.name});
            }
            return [index,leaves];
    }
    
